{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:base"
    ],
    "regexManagers": [
        {
            "fileMatch": [
                "testdata/Pkgfile"
            ],
            "matchStrings": [
                "# renovate: datasource=(?<datasource>.*?) depName=(?<depName>.*?)?\\s.*_version:\\s*(?<currentValue>.*)"
            ],
            "extractVersionTemplate": "^binutils-(?<version>.*)$",
            "versioningTemplate": "regex:^(?<major>\\d+)(\\_(?<minor>\\d+))$"
        }
    ]
}
